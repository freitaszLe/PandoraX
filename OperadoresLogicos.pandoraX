a = inter(summon.x(<Digite 1 para verdadeiro, 0 para falso: >))
b = inter(summon.x(<Digite 1 para verdadeiro, 0 para falso: >))

pandora.expose(<Teste de operadores lógicos>)

when a and b {
    pandora.expose(<Ambos são verdadeiros>)
}
whenever {
    pandora.expose(<Pelo menos um é falso>)
}

when a or b {
    pandora.expose(<Pelo menos um é verdadeiro>)
}

when not a {
    pandora.expose(<A é falso>)
}